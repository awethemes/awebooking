{
  "private": true,
  "main": "gulpfile.js",
  "scripts": {
    "makeicon": "node bin/make-iconfont.js",
    "watch": "export DISABLE_NOTIFIER=true && gulp watch"
  },
  "copyFiles": {
    "sweetalert2": [
      "node_modules/sweetalert2/dist/sweetalert2*.{js,css}"
    ],
    "waypoints": [
      "node_modules/waypoints/lib/jquery.waypoints*.js"
    ],
    "flatpickr": [
      "node_modules/flatpickr/dist/flatpickr*.{js,css}",
      "node_modules/flatpickr/dist/plugins/*Plugin.js",
      "node_modules/flatpickr/dist/themes/confetti.css"
    ],
    "moment": [
      "node_modules/moment/moment.js",
      "node_modules/moment/min/moment.min.js"
    ],
    "selectize": [
      "node_modules/selectize/dist/css/selectize.css",
      "node_modules/selectize/dist/js/standalone/selectize*.js"
    ],
    "tippy.js": [
      "node_modules/tippy.js/dist/tippy.css",
      "node_modules/tippy.js/dist/tippy.js",
      "node_modules/tippy.js/dist/tippy.min.js"
    ]
  },
  "dependencies": {
    "flatpickr": "^4.4.4",
    "moment": "^2.22.0",
    "selectize": "^0.12.4",
    "sweetalert2": "^7.18.0",
    "tippy.js": "^2.5.0",
    "waypoints": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "browser-sync": "^2.23.6",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^8.0.0-beta",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-plumber": "^1.2.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-wp-pot": "^2.2.0",
    "icon.font": "^1.1.0"
  }
}
