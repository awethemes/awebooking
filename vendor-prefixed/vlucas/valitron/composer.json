{
    "name": "vlucas\/valitron",
    "type": "library",
    "description": "Simple, elegant, stand-alone validation library with NO dependencies",
    "keywords": [
        "validation",
        "validator",
        "valid"
    ],
    "homepage": "https:\/\/github.com\/vlucas\/valitron",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Vance Lucas",
            "email": "vance@vancelucas.com",
            "homepage": "https:\/\/www.vancelucas.com"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "phpunit\/phpunit": ">=4.8.35"
    },
    "suggest": {
        "ext-mbstring": "It can support the multiple bytes string length."
    },
    "autoload": {
        "psr-4": {
            "AweBooking\\Vendor\\Valitron\\": "src\/Valitron"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AweBooking\\Vendor\\Valitron\\": "tests\/Valitron"
        }
    },
    "scripts": {
        "test": ".\/vendor\/bin\/phpunit"
    }
}