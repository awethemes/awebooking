@font-face {
	font-family: "{{fontName}}";
	font-style: normal;
	font-weight: normal;
	src: {{{src}}};
}

.{{baseSelector}} {
	font-family: {{fontName}};
	-moz-osx-font-smoothing: grayscale;
	-webkit-font-smoothing: antialiased;
	display: inline-block;
	font-style: normal;
	font-weight: 400;
	font-variant: normal;
	text-rendering: optimizeLegibility;
	line-height: 1;
	vertical-align: middle;
	speak: none;
}

{{#each codepoints}}
.{{../classPrefix}}{{@key}}:before {
	content: "\\{{this}}";
}
{{/each}}
